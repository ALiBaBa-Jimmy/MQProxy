#!/bin/bash

MAKE_PLATFORM=SOLARIS
PRODUCT=XW_SS
CPU=PENTIUM
MICR_BASE=/usr
SRC_ROOT=/home/wul
CPS_ROOT=/home/wul/xcps_v3000_dev/xcps
V3000_ROOT=$CPS_ROOT/cpsv3000
RULES_ROOT=$CPS_ROOT/cpsrules
OBJS_DIR=$V3000_ROOT/obj
ENABLE_PR_XOS_OUT=xossol
WARNING=0
DEBUG=1
XOS_NEED_MAIN=1
XOS_MDLMGT=1
XOS_TELNETD=1
XOS_FTP_CLIENT=1
BIG_POLLING_FD_SETSIZE=1
XOS_ModMem_Check=1
XOS_EW_START=1
XOS_HASH_DEBUG=1
XOS_ARRAY_DEBUG=1
INPUT_PAR_CHECK=1
export MAKE_PLATFORM PRODUCT CPU MICR_BASE SRC_ROOT CPS_ROOT V3000_ROOT RULES_ROOT OBJS_DIR ENABLE_PR_XOS_OUT
export WARNING DEBUG XOS_NEED_MAIN XOS_MDLMGT XOS_TELNETD XOS_FTP_CLIENT
export BIG_POLLING_FD_SETSIZE

XOS_PATH_ENV=/export/home/went/xcps_v3000_xj/xcps/cpsv3000/obj/debug/xossol
PATH=$PATH:/export/home/went/xcps_v3000_xj/xcps/cpsv3000/obj/debug/:
export XOS_PATH_ENV PATH
